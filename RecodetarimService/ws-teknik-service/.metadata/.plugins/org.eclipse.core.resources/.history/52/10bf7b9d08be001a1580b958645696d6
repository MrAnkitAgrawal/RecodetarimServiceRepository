CREATE TABLE `forms` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `form_no` int(11) NOT NULL DEFAULT '0',
  `form_name` varchar(100) COLLATE utf8_bin NOT NULL DEFAULT '',
  `jasper_form_name` varchar(100) COLLATE utf8_bin NOT NULL DEFAULT '',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=5 DEFAULT CHARSET=utf8 COLLATE=utf8_bin;