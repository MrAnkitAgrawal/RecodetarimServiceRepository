package com.techie.recodetarim.domain.entities;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Getter;
import lombok.Setter;
import lombok.ToString;

@Entity
@Table(name = "formsdetay")
@ToString(exclude = "forms")
public class FormsDetay implements Serializable {
	private static final long serialVersionUID = -1315988474181676605L;

	@Id
	@Column(name = "id")
	@Getter
	@Setter
	private Long id;

	@Column(name = "form_no")
	@Getter
	@Setter
	private Long formNo;

	@Column(name = "type")
	@Getter
	@Setter
	private String type;

	@Column(name = "default_value")
	@Getter
	@Setter
	private String defaultValue;

	@Column(name = "not_null")
	@Getter
	@Setter
	private int notNull;

	@Column(name = "title")
	@Getter
	@Setter
	private String title;

	protected FormsDetay() {

	}

	public FormsDetay(Long id, Long formNo, String type, String defaultValue, int notNull, String title) {
		this.id = id;
		this.formNo = formNo;
		this.type = type;
		this.defaultValue = defaultValue;
		this.notNull = notNull;
		this.title = title;
	}
}
